<!DOCTYPE HTML>
<html>
<head>
	<meta charset="utf-8" />
	<title>Martin Urbano</title>
	<link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
	<link rel="stylesheet" href="@Url.Content("~/Content/arcana/css/5grid/core.css")">
	<link rel="stylesheet" href="@Url.Content("~/Content/arcana/css/5grid/core-desktop.css")">
	<link rel="stylesheet" href="@Url.Content("~/Content/arcana/css/5grid/core-1200px.css")">
	<link rel="stylesheet" href="@Url.Content("~/Content/arcana/css/5grid/core-noscript.css")">
	<link rel="stylesheet" href="@Url.Content("~/Content/arcana/css/style.css")">
	<link rel="stylesheet" href="@Url.Content("~/Content/arcana/css/style-desktop.css")">
</head>
<body>
	<!-- Header -->
	<div id="header-wrapper">
		<header class="6grid-layout" id="site-header">
			<div class="row">
				<div class="12u">
					<div class="mobileUI-site-name" id="logo">
						<h2>Martin Urbano</h2>
					</div>
					<nav class="mobileUI-site-nav" id="site-nav">
						<ul>
							<li class="current_page_item"><a href="@Url.Action("Index")">@TextResources.ResourceMartin.MenuHome</a></li>

							<li><a href="@Url.Action("Book")">@TextResources.ResourceMartin.MenuBook</a></li>
							<li><a href="@Url.Action("About")">@TextResources.ResourceMartin.MenuAbout</a></li>
							<li><a href="@Url.Action("Index", "Home")">@TextResources.ResourceMartin.MenuIBO</a></li>
						</ul>
					</nav>
				</div>
			</div>
		</header>
	</div>
	<!-- Main -->
	<div id="main-wrapper" class="subpage">
		@RenderBody()
	</div>
	<!-- Footer -->
	<div id="footer-wrapper">
		<footer class="4grid-layout" id="site-footer">
			<div class="row">
				<div class="3u">
					<section class="first">
						<h2>Ipsum et phasellus</h2>
						<ul class="link-list">
							<li><a href="#">Mattis et quis rutrum sed accumsan</a>
							<li><a href="#">Suspendisse amet varius nibh</a>
							<li><a href="#">Suspenddapibus amet mattis quis</a>
						</ul>
					</section>
				</div>
				<div class="3u">
					<section>
						<h2>Lorem mattis dolor</h2>
						<ul class="link-list">
							<li><a href="#">Duis neque nisi dapibus sed</a>
							<li><a href="#">Suspenddapibus amet mattis quis</a>
						</ul>
					</section>
				</div>
			</div>
			<div class="row">
				<div class="12u">
					<div class="divider"></div>
				</div>
			</div>
			<div class="row">
				<div class="12u">
					<div id="copyright">&copy; Martin Urbano. All rights reserved. | Design: Interactive Labs - <a target="_blank" href="http://html5up.net">HTML5 Up!</a> | Images: <a target="_blank" href="http://fotogrph.com">Fotogrph</a> </div>
				</div>
			</div>
		</footer>
	</div>
	@* Scripts *@
	<script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
	<script src="http://code.jquery.com/jquery-migrate-1.1.1.js"></script>
	<script src="~/Content/arcana/css/5grid/init.js?use=mobile,desktop,1000px&amp;mobileUI=1&amp;mobileUI.theme=none&amp;mobileUI.titleBarHeight=60&amp;mobileUI.openerWidth=52"></script>
</body>
</html>
